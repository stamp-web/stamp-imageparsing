{
    "name": "stamp-imageparsing",
    "productName": "Stamp Image Bursting Application",
    "description": "Next-Generation Image Bursting Application",
    "version": "3.0.1",
    "copyright": "Copyright © 2021 Jason Drake.  All Rights Reserved.",
    "bugs": {
        "url": "https://github.com/jadrake75/stamp-imageparsing/issues"
    },
    "repository": "github:jadrake75/stamp-imageparsing",
    "license": "Apache 2.0",
    "author": "Jason Drake (jadrake75@gmail.com)",
    "main": "main.js",
    "scripts": {
        "build": "au build",
        "watch": "au run --watch",
        "prewin32": "au build",
        "win32": "electron-builder --x64",
        "start": "electron .",
        "postinstall": "electron-builder install-app-deps",
        "rebuild-modules": "electron-rebuild",
        "package-win": "electron-packager . stamp-imageparsing --overwrite --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Stamp Image Bursting Application\""
    },
    "build": {
        "appId": "com.drakeserver.stamp-imageparsing",
        "copyright": "Copyright � 2019 Jason Drake (jadrake75@gmail.com)",
        "win": {
            "icon": "assets/icons/win/icon.ico",
            "target": "nsis",
            "files": [
                "assets/fonts/stamp-web/css/stamp-web-embedded.css",
                "assets/images${/*}",
                "assets/svg${/*}",
                "LICENSE",
                "main.js",
                "index.html",
                "scripts${/*}",
                "lib{/*}",
                "node_modules/bootstrap/dist/css/bootstrap.min.css",
                "platform${/*}",
                "locales${/*}",
                "!assets/sketches",
                "!build${/*}",
                "!test${/*}",
                "!target${/*}",
                "!node_modules/aurelia*/${/*}",
                "!node_modules/bootstrap/js/${/*}",
                "!node_modules/bootstrap/scss/${/*}",
                "!node_modules/bootstrap/dist/js/${/*}",
                "!node_modules/bootstrap/dist/css/bootstrap-grid*",
                "!node_modules/bootstrap/dist/css/bootstrap-reboot*",
                "!node_modules/bootstrap/dist/css/bootstrap-css*",
                "!nbproject${/*}",
                "!aurelia_project${/*}",
                "!java${/*}",
                "!build.xml",
                "!pom.xml",
                "!gulpfile.js",
                "!karma.conf.js",
                "!.babelrc.js",
                "!.editorconfig",
                "!.eslintrc.json",
                "!.gitignore",
                "!src${/*}"
            ]
        },
        "asar": true,
        "asarUnpack": [
            "lib/*"
        ],
        "icon": "assets/icons/win/icon.ico",
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "artifactName": "stamp-imageparsing-install.${ext}",
            "installerSidebar": "images/sidebar.bmp",
            "installerLanguages": [
                "en-US"
            ]
        },
        "directories": {
            "output": "release-builds",
            "buildResources": "assets"
        },
        "extraFiles": [
            "lib/**"
        ]
    },
    "dependencies": {
        "@fnando/sparkline": "0.3.10",
        "@stomp/stompjs": "github:stomp-js/stompjs",
        "aurelia-fetch-client": "1.8.2",
        "bootstrap": "4.5.2",
        "browser-sync": "2.26.12",
        "changedpi": "1.0.4",
        "file-api": "0.10.4",
        "filereader": "0.10.3",
        "lodash": "4.17.20",
        "mime": "1.4.1",
        "requirejs": "2.3.6",
        "sharp": "^0.27.1",
        "sockjs-client": "1.5.0",
        "text": "github:requirejs/text#latest",
        "uid-generator": "2.0.0"
    },
    "devDependencies": {
        "@babel/core": "7.11.4",
        "@babel/plugin-proposal-class-properties": "7.3.0",
        "@babel/plugin-proposal-decorators": "7.3.0",
        "@babel/plugin-transform-modules-amd": "7.2.0",
        "@babel/plugin-transform-modules-commonjs": "7.2.0",
        "@babel/polyfill": "7.2.5",
        "@babel/preset-env": "7.3.1",
        "@babel/register": "7.0.0",
        "aurelia-animator-css": "1.0.4",
        "aurelia-bootstrapper": "2.3.3",
        "aurelia-cli": "2.0.0",
        "aurelia-dialog": "2.0.0",
        "aurelia-i18n": "3.1.1",
        "aurelia-testing": "1.0.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.1.0",
        "babel-plugin-istanbul": "5.1.1",
        "electron": "11.1.1",
        "electron-builder": "22.8.1",
        "electron-packager": "15.1.0",
        "electron-rebuild": "2.0.1",
        "electron-winstaller": "4.0.1",
        "gulp": "4.0.2",
        "gulp-babel": "8.0.0",
        "gulp-changed-in-place": "2.3.0",
        "gulp-eslint": "5.0.0",
        "gulp-htmlmin": "3.0.0",
        "gulp-notify": "3.2.0",
        "gulp-plumber": "1.2.1",
        "gulp-rename": "1.4.0",
        "gulp-sass": "4.0.2",
        "gulp-shell": "0.6.5",
        "gulp-sourcemaps": "2.6.5",
        "http-proxy-middleware": "^0.19.1",
        "i18next": "11.3.3",
        "i18next-xhr-backend": "1.5.1",
        "jasmine-core": "2.99.1",
        "jquery": "3.5.1",
        "karma": "5.1.1",
        "karma-babel-preprocessor": "8.0.1",
        "karma-chrome-launcher": "1.0.1",
        "karma-coverage": "1.1.2",
        "karma-firefox-launcher": "1.3.0",
        "karma-jasmine": "^1.1.2",
        "karma-junit-reporter": "1.2.0",
        "karma-remap-istanbul": "0.6.0",
        "karma-requirejs": "1.1.0",
        "node-abi": "^2.19.3",
        "popper.js": "1.16.1",
        "reflect-metadata": "^0.1.13",
        "require-dir": "1.2.0",
        "yargs": "15.4.1"
    }
}
